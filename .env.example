# AWS credentials for CDK deployment - DO NOT COMMIT
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=

# Custom domain (fixes CloudFront 403 when accessing via izan.io)
# ACM certificate must be in us-east-1 - create in AWS Console: Certificate Manager (us-east-1)
IZAN_DOMAIN_CERTIFICATE_ARN=
# Route53 hosted zone ID (optional, for auto A records; if empty, configure DNS manually)
IZAN_HOSTED_ZONE_ID=

# Namecheap API keys for domain availability check
NAMECHEAP_API_USER=
NAMECHEAP_API_KEY=
NAMECHEAP_CLIENT_IP=